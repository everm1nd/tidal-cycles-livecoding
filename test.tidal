d1 $ sound "[bd:4]*2" # gain "1.2"

d2 $ sound "sd(4,12)" # n (irand 10) # silence

d3 $ sound "hh(6,8)"

d4 $ up "0 1 2 3 4 5 6" # sound "hoover"

d5 $ sound "amencutup(7,12,5)" * (rand * 10 + 1)

d6 $ every 4 (fast 2) $ s "fm" # n (irand 30) # speed "0.5"

d6 $ every 3 (fast 2) $ s "amencutup*8" # n (irand 30)

d7 $ every 4 (fast 2) $ s "hoover*2" # n (irand 30) # gain "0.8"

d8 $ s "[gabba sd]*2"

d9 $ s "hh:8*8"

d10 $ speed "[1 1.5 0.5 2]/8" # s "fm:14" # gain "0.85"

d10 $ speed "[1 0.5, 1.5 2 3 1]/8" # s "fm:14" # gain "0.85"

d10 $ every 4 (|+ speed "1") $ s "fm:14" # gain "0.85"

d11 $ up "0 1 3*2 0 0*2 0 0*2 0" # sound "arpy" # speed "1"
